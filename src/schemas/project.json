{
  "Main": {
    "title": {
      "type": "Text",
      "config": {
        "label": "Title"
      }
    },
    "uid": {
      "type": "UID",
      "config": {
        "label": "UID"
      }
    },
    "thumbnail": {
      "type": "Image",
      "config": {
        "constraint": {
          "width": 750
        },
        "thumbnails": [],
        "label": "Thumbnail"
      }
    },
    "full_image": {
      "type": "Image",
      "config": {
        "constraint": {
          "width": null
        },
        "thumbnails": [],
        "label": "Full Image"
      }
    },
    "body": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, preformatted, heading2, heading3, heading4, heading5, heading6, strong, em, hyperlink, embed, list-item, o-list-item",
        "allowTargetBlank": true,
        "label": "Body"
      }
    },
    "tools": {
      "type": "Text",
      "config": {
        "label": "Tools",
        "placeholder": "Enter tools used, e.g.  GatsbyJS, Prismic.io, Netlify"
      }
    },
    "url": {
      "type": "Link",
      "config": {
        "allowTargetBlank": true,
        "label": "URL"
      }
    },
    "source": {
      "type": "Link",
      "config": {
        "allowTargetBlank": true,
        "label": "Source"
      }
    }
  }
}
