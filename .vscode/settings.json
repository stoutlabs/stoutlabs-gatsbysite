{
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "markdown.styles": [
    "preview-styles.css"
  ],
  "prettier.disableLanguages": [
    "javascript",
    "javascriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
